{
  "buildCommand": "bun run build",
  "installCommand": "curl -fsSL https://bun.sh/install | bash && bun install",
  "framework": null,
  "functions": {
    "api/index.ts": {
      "runtime": "edge"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/index"
    }
  ]
}
